<div class="row">
  <div class="col">
    <% @results.each do |result| %>
      <%= link_to "#{result.first_name} #{result.last_name} <#{result.email}>", user_path(result) %><br />
      <strong><%= result.company %></strong><br />
      <em><%= result.company_description %></em><br />
      <br />
    <% end %>
  </div>
</div>
